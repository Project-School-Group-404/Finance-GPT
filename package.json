{
  "name": "fgpt-fullstack",
  "version": "1.0.0",
  "description": "Full-stack FGPT application",
  "scripts": {
    "backend": "cd server && npm install && npx prisma generate",
    "requirements": "cd FastAPI_Server && pip install -r requirements.txt",
    "frontend": "cd client && npm install",
    "setup": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run requirements\"",
    "dev": "concurrently \"npm run server:start\" \"npm run client:dev\"",
    "fastapi": "cd FastAPI_Server && uvicorn Graph:app --host 127.0.0.1 --port 8000",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm run dev",
    "start": "concurrently \"npm run dev\" \"npm run fastapi\""
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "express-session": "^1.18.1",
    "node": "^22.17.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
