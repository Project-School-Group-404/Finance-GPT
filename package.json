{
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "llmwhisperer-client": "^2.3.0"
  },
  "scripts": {
    "backend": "cd frontend/server && npx prisma generate",
    "requirements": "pip install -r requirements.txt",
    "frontend": "cd frontend && npm install && cd client && npm install",
    "setup": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run requirements\"",
    "dev": "npm --prefix frontend start",
    "fastapi":"uvicorn App:app --host 127.0.0.1 --port 8000",
    "start": "concurrently \"npm run dev\" \"npm run fastapi\""
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
